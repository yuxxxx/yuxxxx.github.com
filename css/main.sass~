/*楽曲の達成状況に関する設定*/
#music
	.auto:after
		:content '(auto)'
	.difficulty:after
		:content ':Lv' attr(data-level) ' ' attr(data-rating) '(' attr(data-score) ')'

/*達成情報の履歴に関する設定*/
#news .news
	.achieve:after
		:content '(' attr(data-count) '曲目)'
	.title 
		:overflow hidden
		:text-overflow ellipsis
		&:before
			:content attr(data-difficulty) ':'
